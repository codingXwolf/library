r() {\n    const myLibrary = this.state.library.map((book) => {\n      return (\n        <div key={book.id}>\n          <Grid.Column>\n            <BookCard\n              data={book}\n              delBook={this.delBook}\n              readStatusHandler={this.readStatusHandler}\n            ></BookCard>\n          </Grid.Column>\n        </div>\n      );\n    });\n\n    return (\n      <div>\n        <BookForm addBook={this.addBook} />\n        <br />\n        <br />\n        <Grid columns={3} celled=\"internally\" divided>\n          <GridRow>\n            <CardGroup>{myLibrary}</CardGroup>\n          </GridRow>\n        </Grid>\n      </div>\n    );\n  }\n}\n\nexport default Library;\n","/Users/jaimeortiz/projects/the-odin-project/projects/library/library-react/src/components/BookCard.js",[],"/Users/jaimeortiz/projects/the-odin-project/projects/library/library-react/src/components/BookForm.js",[],"/Users/jaimeortiz/projects/the-odin-project/projects/library/library-react/src/components/FormModal.js",[],{"ruleId":"33","replacedBy":"34"},{"ruleId":"35","replacedBy":"36"},{"ruleId":"37","severity":1,"message":"38","line":5,"column":10,"nodeType":"39","messageId":"40","endLine":5,"endColumn":16},{"ruleId":"37","severity":1,"message":"41","line":6,"column":8,"nodeType":"39","messageId":"40","endLine":6,"endColumn":17},{"ruleId":"42","severity":1,"message":"43","line":65,"column":35,"nodeType":"44","messageId":"45","endLine":65,"endColumn":37},"no-native-reassign",["46"],"no-negated-in-lhs",["47"],"no-unused-vars","'Header' is defined but never used.","Identifier","unusedVar","'FormModal' is defined but never used.","array-callback-return","Array.prototype.map() expects a return value from arrow function.","ArrowFunctionExpression","expectedInside","no-global-assign","no-unsafe-negation"]